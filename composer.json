{
    "name": "kabunx/hydrate",
    "description": "entity or xml hydrate",
    "license": "MIT",
    "authors": [
        {
            "name": "jonathan",
            "email": "jianyang1209@outlook.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "nesbot/carbon": "^2.50"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Kabunx\\Hydrate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kabunx\\Hydrate\\Test\\": "tests"
        }
    },
    "extra": {},
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
